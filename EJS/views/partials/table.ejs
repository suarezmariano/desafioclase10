<% if(productos.length !==0) { %>
<div>
  <table>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Thumbnail</th>
    </tr>
    <% for (const producto in productos) {%>
    <tr>
      <td><%= productos[producto].title %></td>
      <td><%= productos[producto].price %></td>
      <td>
        <img src="<%= productos[producto].thumbnail %>" alt="image" />
      </td>
    </tr>
    <% } %>
  </table>
</div>
<% } else{ %>
<p>No hay productos cargados</p>
<% }%>
